{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DashboardDb": "Data Source=.;Initial Catalog=ExamleETL;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Data Source=.;Initial Catalog=ExamleETL;Trusted_Connection=True;",
          "tableName": "Log",
          "schemaName": "adm",
          "autoCreateSqlTable": false,
          "columnOptionsSection": {
            "primaryKeyColumnName": "Id",
            "removeStandardColumns": [ "MessageTemplate", "Properties" ],
            "additionalColumns": [
              {
                "ColumnName": "Server",
                "DataType": "varchar",
                "DataLength": 50
              },
              {
                "ColumnName": "Application",
                "DataType": "varchar",
                "DataLength": 50
              }
            ]
          }
        }
      }
    ]
  }
}
